#
# Definition for singly-linked list.
# class ListNode:
#     def __init__(self, x):
#         self.val = x
#         self.next = None

class Solution:
    def removeNthFromEnd(self, head, n):
        """
        :type head: ListNode
        :type n: int
        :rtype: ListNode
        """
        a=head
        b=head
        le=1
        while a.next!=None:
            a=a.next
            le+=1
        if le==1:
            return None
        a=head
        if n==1:
            for i in range(le-2):
                a=a.next
            a.next=None
            return head
        a=head
        for i in range(n-1):
            a=a.next
        while a.next!=None:
            a=a.next
            b=b.next
        b.val=b.next.val
        b.next=b.next.next
        return head
#
#
