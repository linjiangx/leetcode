class Solution(object):
    def removeNthFromEnd(self, head, n):
        """
        :type head: ListNode
        :type n: int
        :rtype: ListNode
        """
        ap=head
        bp=head
        for i in range(n):
            bp=bp.next
        if bp==None:
            return head.next       
        while bp.next:
            ap=ap.next
            bp=bp.next
        ap.next=ap.next.next
        return head
