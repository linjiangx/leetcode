#判断List是否有任意三个元素按照递增排列
class Solution:
    def increasingTriplet(self, nums):
        low, res = float('inf'), [float('inf'), float('inf')]
        for i in range(len(nums)):
            if nums[i] > res[-1]: return True
            if nums[i] > low: res = [low, nums[i]]
            low = min(low, nums[i])
        return False

#这个方法可以找更长递增排序
class Solution(object):
    def increasingTriplet(self, nums):
        sequence = []
        for num in nums:
            for seq_index, seq_num in enumerate(sequence):
                if seq_num >= num:
                    sequence[seq_index] = num
                    break
            else:
                sequence.append(num)
            if len(sequence) >= 3:
                return True
        return False
