#
class Solution(object):
    def findDuplicate(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        return (sum(nums) - sum(set(nums)))/(len(nums)-len(set(nums)))
        
#
class Solution(object):
    def findDuplicate(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if nums:
            store = 0
            for n in nums:
                if 1 << n & store:
                    return n
                store = store | 1 << n
        return -1
