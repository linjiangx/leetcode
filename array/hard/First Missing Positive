#
class Solution(object):
    def firstMissingPositive(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        length = len(nums)
        k = 0
        for i in range(k):
            num = abs(nums[i])
            
            if nums[num-1] > 0:
                nums[num-1] *= -1
        
        for i in range(k):
            if nums[i] >= 0:
                return i+1
        
        return k+1
