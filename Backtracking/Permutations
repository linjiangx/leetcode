#
class Solution:
    def permute(self, nums):
        """
        :type nums: List[int]
        :rtype: List[List[int]]
        """
        if not nums:
            return []
        if len(nums) == 1:
            return [nums]
        prev, cur = [[nums[0]]], []
        for i in range(1,len(nums)):
            for j in range(i+1):
                for ele in prev:
                    cur.append(ele[:j]+[nums[i]]+ele[j:]) 
            prev, cur = cur, []
        return prev
