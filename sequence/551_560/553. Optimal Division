#
#
class Solution:
    def optimalDivision(self, nums):
        """
        :type nums: List[int]
        :rtype: str
        """
        n = len(nums)
        if n == 1: return str(nums[0])
        if n == 2: return str(nums[0]) + '/' + str(nums[1])
        return str(nums[0]) + '/(' + '/'.join(map(str, nums[1:])) + ')'
