#
#
class Solution:
    def nextPermutation(self, nums):
        """
        :type nums: List[int]
        :rtype: void Do not return anything, modify nums in-place instead.
        """
        if len(nums) <= 1:
            return
        flag = 0
        for i in range(len(nums) - 1, 0, -1):
            if nums[i] > nums[i - 1]:
                peak = i
                flag = 1
                break
        if not flag:
            nums.sort()
        else:
            if peak == len(nums) - 1:
                nums[peak], nums[peak - 1] = nums[peak - 1], nums[peak]
            else:
                for i in range(len(nums) - 1, peak - 1, -1):
                    if nums[i] > nums[peak - 1]:
                        nums[i], nums[peak - 1] = nums[peak - 1], nums[i]
                        break
                nums[peak:] = nums[peak:][::-1]
        
        
