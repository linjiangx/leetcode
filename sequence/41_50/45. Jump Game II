##
#
class Solution:
    def jump(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums) <= 1:
            return 0
        left, right = 0, nums[0]
        time = 1
        while right < len(nums) - 1:
            left, right = right, max(i + nums[i] for i in range(left, right + 1))
            time += 1
        return time
            
                
