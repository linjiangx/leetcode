#
#
class Solution:
    def firstMissingPositive(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        point = 0
        nums_len = len(nums)
        for i in range(nums_len):
            if 0 < nums[i] <= nums_len:
                nums[point] = nums[i]
                point += 1
        for i in range(point):
            if nums[abs(nums[i]) - 1] > 0:
                nums[abs(nums[i]) - 1] *= -1
        for i in range(point):
            if nums[i] > 0:
                return i + 1
        return point + 1
            
