#Runtime: 76 ms, faster than 40.26% of Python3 online submissions for Remove Duplicates from Sorted Array.
#
class Solution:
    def removeDuplicates(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        point = 1
        if len(nums) <= 1:
            return len(nums)
        for i in range(1, len(nums)):
            if nums[i] == nums[i - 1]:
                continue
            else:
                nums[point] = nums[i]
                point += 1
        return point
