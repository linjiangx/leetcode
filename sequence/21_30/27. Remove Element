#
#
class Solution:
    def removeElement(self, nums, val):
        """
        :type nums: List[int]
        :type val: int
        :rtype: int
        """
        point = 0
        for i in range(len(nums)):
            if nums[i] == val:
                continue
            else:
                nums[point] = nums[i]
                point += 1
        return point
