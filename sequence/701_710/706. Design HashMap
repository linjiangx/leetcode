#
#
class MyHashMap:

    def __init__(self):
        self.address_map = [-1] * 1000000

    def put(self, key: 'int', value: 'int') -> 'None':
        self.address_map[key] = value

    def get(self, key: 'int') -> 'int':
        return self.address_map[key]

    def remove(self, key: 'int') -> 'None':
        self.address_map[key] = -1
