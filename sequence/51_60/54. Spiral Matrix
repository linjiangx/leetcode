#
#
class Solution:
    def spiralOrder(self, matrix):
        """
        :type matrix: List[List[int]]
        :rtype: List[int]
        """
        res = []
        while matrix:
            a = matrix.pop(0)
            res.extend(a)
            for i in range(len(matrix)): 
                matrix[i][:] = (matrix[i])[::-1]  
            matrix = list(map(list, zip(*matrix)))
        return res
