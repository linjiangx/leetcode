#
#
class Solution:
    def generateMatrix(self, n):
        """
        :type n: int
        :rtype: List[List[int]]
        """
        res = [[1 for i in range(n)] for j in range(n)]
        left, right = 0, n - 1
        up, down = 0, n - 1
        num = 1
        while left <= right and up <= down:
            for i in range(left, right + 1):
                res[up][i] = num
                num += 1
            up += 1
            for i in range(up, down + 1):
                res[i][right] = num
                num += 1
            right -= 1
            for i in range(right, left - 1, -1):
                res[down][i] = num
                num += 1
            down -= 1
            for i in range(down, up - 1, -1):
                res[i][left] = num
                num += 1
            left += 1
        return res
