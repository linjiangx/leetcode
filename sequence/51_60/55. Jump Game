#
#
class Solution:
    def canJump(self, nums):
        """
        :type nums: List[int]
        :rtype: bool
        """
        left, right = 0, nums[0]
        while right < len(nums):
            new_left = right
            new_right = max(i + nums[i] for i in range(left, right + 1))
            if new_right >= len(nums) - 1:
                return True
            if new_right == right:
                return False
            left, right = new_left, new_right
        return True
