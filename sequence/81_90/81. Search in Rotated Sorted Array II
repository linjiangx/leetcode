#
#
class Solution:
    def search(self, nums: 'List[int]', target: 'int') -> 'bool':
        if len(nums) == 0:
            return False
        elif len(nums) == 1:
            return nums[0] == target
        else:
            left, right = 0, len(nums) - 1
            while left <= right:
                mid = (left + right) // 2
                if nums[mid] == target:
                    return True
                if nums[left] <= target < nums[mid]:
                    right = mid - 1
                elif nums[mid] < target <= nums[right]:
                    left = mid + 1
                elif nums[left] <= nums[mid]:
                    left += 1
                else:
                    right -= 1
            return False
