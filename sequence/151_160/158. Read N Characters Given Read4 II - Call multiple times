#
#
class Solution(object):
    def __init__(self):
        self.q = []
    
    def read(self, buf, n):
        """
        :type buf: Destination buffer (List[str])
        :type n: Maximum number of characters to read (int)
        :rtype: The number of characters read (int)
        """
        i = 0
        while i < n:
            if self.q:
                buf[i] = self.q.pop(0)
                i += 1
            else:
                buf4 = [''] * 4
                v = read4(buf4)
                if v == 0:
                    break
                self.q += buf4[:v]
        return i
