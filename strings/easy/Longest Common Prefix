#求最长前缀，注意三个边界：1.strs为空，2.后面的字符串比第一个短，3.当出现不一样的字符时break

#
class Solution:
    def longestCommonPrefix(self, strs):
        """
        :type strs: List[str]
        :rtype: str
        """
        if not strs:
            return ''
        res=strs[0]
        for st in strs:
            k=0
            for i in range(min(len(res),len(st))):
                if res[i]==st[i]:
                    k+=1
                else:
                    break
            res=res[0:k]
        return res
            
