#寻找峰值，高峰或者低峰，两边为负无穷大
class Solution:
    def findPeakElement(self, nums):
        """
        :type nums: List[int]
        :rtype: int
        """
        if len(nums)==1:
            return 0
        if nums[0]>nums[1]:
            return 0
        for i in range(1,len(nums)-1):
            if (nums[i-1]-nums[i])*(nums[i]-nums[i+1])<0:
                return i
        if nums[len(nums)-1]>nums[len(nums)-2]:
            return len(nums)-1

