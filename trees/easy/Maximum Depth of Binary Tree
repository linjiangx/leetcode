#
return (1 + max(self.maxDepth(root.left), self.maxDepth(root.right))) if root else 0


#
       if root is None:
            return 0
        queue = [root]
        ans   = 0
        while queue:
            n = len(queue)
            while n > 0:
                x = queue.pop(0)
                if x.left is not None:
                    queue.append(x.left)
                if x.right is not None:
                    queue.append(x.right)    
                n = n - 1 
            ans = ans + 1
        return ans
